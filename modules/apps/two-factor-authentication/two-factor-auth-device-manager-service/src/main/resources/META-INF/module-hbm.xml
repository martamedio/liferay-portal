<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.bemis.portal.twofa.device.manager.model.Device" />
	<import class="com.bemis.portal.twofa.device.manager.model.DeviceCode" />
	<class name="com.bemis.portal.twofa.device.manager.model.impl.DeviceImpl" table="Bemis_Device">
		<id name="deviceId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="portalUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="portalUserName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="deviceIP" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="browserName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="osName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="verified" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="tempDevice" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="com.bemis.portal.twofa.device.manager.model.impl.DeviceCodeImpl" table="Bemis_DeviceCode">
		<id name="deviceCodeId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="portalUserId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="portalUserName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="deviceCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="deviceIP" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="validationCode" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
</hibernate-mapping>