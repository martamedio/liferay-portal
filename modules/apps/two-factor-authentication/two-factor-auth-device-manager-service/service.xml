<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.bemis.portal.twofa.device.manager">
	<author>Prathima Shreenath</author>
	<namespace>Bemis</namespace>

	<entity local-service="true" name="Device" remote-service="false">
		<column name="deviceId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="portalUserId" type="long" />
		<column name="portalUserName" type="String" />
		<column name="emailAddress" type="String" />
		<column name="deviceIP" type="String" />
		<column name="browserName" type="String" />
		<column name="osName" type="String" />
		<column name="verified" type="boolean" />
		<column name="tempDevice" type="boolean" />

		<!-- Finder methods -->

		<finder name="PortalUserId" return-type="Collection">
			<finder-column name="portalUserId" />
		</finder>

		<finder name="PortalUserId_DeviceIP" return-type="Device">
			<finder-column name="portalUserId" />
			<finder-column name="deviceIP" />
		</finder>

		<finder name="Verified_PortalUserId" return-type="Collection">
			<finder-column name="verified" />
			<finder-column name="portalUserId" />
		</finder>

		<finder name="Verified_PortalUserId_DeviceIP" return-type="Device">
			<finder-column name="verified" />
			<finder-column name="portalUserId" />
			<finder-column name="deviceIP" />
		</finder>

		<finder name="TempDevice_PortalUserId_DeviceIP" return-type="Device">
			<finder-column name="tempDevice" />
			<finder-column name="portalUserId" />
			<finder-column name="deviceIP" />
		</finder>
	</entity>

	<entity local-service="true" name="DeviceCode" remote-service="false">
		<column name="deviceCodeId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="portalUserId" type="long" />
		<column name="portalUserName" type="String" />
		<column name="emailAddress" type="String" />
		<column name="deviceCode" type="String" />
		<column name="deviceIP" type="String" />
		<column name="validationCode" type="int" />

		<!-- Finder methods -->

		<finder name="PortalUserId" return-type="DeviceCode">
			<finder-column name="portalUserId" />
		</finder>

		<finder name="EmailAddress" return-type="DeviceCode">
			<finder-column name="emailAddress" />
		</finder>
	</entity>

	<entity local-service="true" name="DeviceManager" remote-service="true" />
</service-builder>